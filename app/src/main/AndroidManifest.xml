<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rolplay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mainlogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mainlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApp">
        <activity android:name=".MenuPersonajesActivity" android:screenOrientation="nosensor"/>
        <activity android:name=".InformacionActivity" android:screenOrientation="nosensor" />
        <activity
            android:name=".ContenedorInicioActivity"
            android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:screenOrientation="nosensor" />
    </application>

</manifest>